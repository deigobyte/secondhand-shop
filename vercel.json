{
  "version": 2,
  "functions": {
    "api/items.js": {
      "maxDuration": 10
    },
    "api/chat.js": {
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/api/items",
      "dest": "/api/items.js"
    },
    {
      "src": "/api/chat",
      "dest": "/api/chat.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
